   for Source_Files use ("abr_huffman.adb", "abr_huffman.ads", "compresser.adb", "decompresser.adb", "lca.adb", "lca.ads", "main.adb", "sda_exceptions.ads", "test_module_abr.adb", "test_module_lca.adb");

   package Builder is
      for Default_Switches ("ada") use ("-s");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatwa", "-gnata", "-g");
